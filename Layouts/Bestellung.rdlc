  <Code>Shared Dim HeaderData As New System.Collections.Generic.Dictionary(Of String, Object)

Public Function SetLayoutXML(ByVal XML As String) As Boolean
  Dim XMLDoc As New System.Xml.XmlDocument()
  XMLDoc.LoadXML(XML)

  Dim XMLNodeList As System.Xml.XmlNodeList
  XMLNodeList = XMLDoc.SelectNodes("//DocumentLayoutSetup/*")

  Try
    For Each element As System.Xml.XmlElement In XMLNodeList
      If Not HeaderData.ContainsKey(element.Name) Then
        HeaderData.Add(element.Name, element.InnerText)
      End If
    Next
  Finally
  End Try

  Return True
End Function

Public Function SetHeaderData(ByVal Key As String, ByVal Value As Object) As Boolean
  If Not HeaderData.ContainsKey(Key) Then
    HeaderData.Add(Key, Value)
    Return True
  Else
    HeaderData(Key) = Value
    Return True
  End If
  Return False
End Function

Public Function GetHeaderData(ByVal Key As String) As Object
  If HeaderData.ContainsKey(Key) Then
    Return HeaderData(Key)
  Else
    Return ""
  End If
End FUnction

Public Function BlankZero(ByVal Value As Decimal)
    if Value = 0 then
        Return ""
    end if
    Return Value
End Function

Public Function BlankPos(ByVal Value As Decimal)
    if Value &gt; 0 then
